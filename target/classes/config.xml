<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- more bean definitions for services go here -->
    <bean class="com.SpringApp.SpringApp1.Student" name="student1">
        <property name="id">
            <value>123</value>
        </property>
        <property name="name">
            <value>Ram</value>
        </property>
        <property name="address">
            <value>Pune</value>
        </property>
    </bean>
    <bean class="com.SpringApp.SpringApp1.Student" name="student2">
        <property name="id">
            <value>234</value>
        </property>
        <property name="name">
            <value>Sham</value>
        </property>
        <property name="address">
            <value>Mumbai</value>
        </property>
    </bean>
</beans>